DJANGO_SETTINGS_MODULE="camac.settings"
DJANGO_DATABASE_NAME="{{ camac_db_dbname }}"
DJANGO_DATABASE_USER="{{ camac_db_username }}"
DJANGO_DATABASE_PASSWORD="{{ camac_db_password }}"
DJANGO_DATABASE_HOST="{{ camac_db_host }}"
DJANGO_DATABASE_PORT="{{ camac_db_port }}"
DJANGO_ALLOWED_HOSTS="{{ camac_python_django_allowed_hosts }}"
DJANGO_SECRET_KEY="{{ camac_python_django_secret_key }}"
DJANGO_MEDIA_ROOT="{{ camac_python_django_media_root }}"
DJANGO_DEFAULT_FROM_EMAIL="{{ camac_python_django_default_from_email }}"
DJANGO_SERVER_EMAIL="{{ camac_python_django_server_email }}"
DJANGO_EMAIL_HOST="{{ camac_python_django_email_host }}"
DJANGO_EMAIL_PORT="{{ camac_python_django_email_port }}"
DJANGO_CLAMD_ENABLED="{{ camac_python_clamd_enabled }}"
DJANGO_CLAMD_TCP_ADDR="{{ camac_python_clamd_tcp_addr }}"
APPLICATION="{{ camac_application }}"
KEYCLOAK_URL="{{ camac_python_keycloak_url }}"
KEYCLOAK_REALM="{{ camac_python_keycloak_realm }}"
KEYCLOAK_CLIENT="{{ camac_python_keycloak_client }}"
CALUMA_URL="{{ camac_caluma_url }}"
DEMO_MODE={{ camac_demo_mode }}

{% if camac_auth_proxy %}
HTTP_PROXY="{{ camac_auth_proxy }}"
HTTPS_PROXY="{{ camac_auth_proxy }}"
{% endif %}
